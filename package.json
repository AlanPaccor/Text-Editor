    "express": "^4.17.1",
    "if-env": "^1.0.4"
  },
  "devDependencies": {
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.20",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4"
  },
  "engines": {
    "node": "16.15.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AlanPaccor/Text-Editor"
  },
